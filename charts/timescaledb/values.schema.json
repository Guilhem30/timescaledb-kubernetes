{
  "components": {
    "schemas": {
      "User": {
        "additionalProperties": {
          "additionalProperties": false,
          "type": "object",
          "properties": {
            "privileges": {
              "additionalProperties": false,
              "type": "object",
              "description": "https://www.postgresql.org/docs/current/sql-createrole.html",
              "properties": {
                "superuser": {
                  "type": "boolean"
                },
                "createdb": {
                  "type": "boolean"
                },
                "login": {
                  "type": "boolean"
                },
                "createrole": {
                  "type": "boolean"
                },
                "replication": {
                  "type": "boolean"
                }
              }
            },
            "password": {
              "additionalProperties": false,
              "type": "object",
              "properties": {
                "valueFrom": {
                  "additionalProperties": false,
                  "type": "object",
                  "properties": {
                    "secretKeyRef": {
                      "additionalProperties": false,
                      "required": [
                        "name",
                        "key"
                      ],
                      "properties": {
                        "name": {
                          "minLength": 1,
                          "type": "string"
                        },
                        "key": {
                          "minLength": 1,
                          "type": "string"
                        }
                      }
                    }
                  }
                },
                "value": {
                  "minLength": 8,
                  "type": "string"
                }
              }
            }
          }
        },
        "type": "object",
        "examples": {
          "referToSelfManagedSecret": {
            "admin": {
              "password": {
                "valueFrom": {
                  "secretKeyRef": {
                    "name": "selfManagedSecret",
                    "key": "admin"
                  }
                }
              }
            }
          },
          "additionalCreateDBUser": {
            "devops": {
              "privileges": [
                "createdb"
              ]
            }
          },
          "plainTextPassword": {
            "admin": {
              "password": {
                "value": "verysecret"
              }
            }
          }
        }
      }
    }
  },
  "title": "Values",
  "additionalProperties": false,
  "$schema": "https://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "spec": {
      "additionalProperties": false,
      "required": [
        "nodes",
        "configurations"
      ],
      "type": "object",
      "properties": {
        "accessNode": {
          "minLength": 1,
          "type": [
            "string",
            "null"
          ]
        },
        "nodes": {
          "additionalProperties": {
            "additionalProperties": false,
            "type": "object",
            "properties": {
              "configuration": {
                "type": "string",
                "example": "default"
              }
            }
          },
          "type": "object"
        },
        "databases": {
          "additionalProperties": {
            "additionalProperties": {
              "type": "string"
            },
            "type": "object",
            "properties": {
              "locale": {
                "minLength": 1,
                "type": "string",
                "example": "C"
              },
              "encoding": {
                "minLength": 3,
                "type": "string",
                "example": "UTF8"
              }
            }
          },
          "type": "object"
        },
        "users": {
          "$ref": "#/components/schemas/User"
        },
        "configurations": {
          "additionalProperties": {
            "additionalProperties": false,
            "type": "object",
            "properties": {
              "image": {
                "minLength": 3,
                "type": "string",
                "example": "timescaledev/timescaledb-ha:pg11-ts1.6-postgis"
              },
              "postgresql": {
                "type": "object",
                "properties": {
                  "version": {
                    "minimum": 11,
                    "type": "integer"
                  },
                  "parameters": {
                    "additionalProperties": {
                      "type": "string"
                    },
                    "type": "object",
                    "example": {
                      "work_mem": "24MB"
                    }
                  }
                }
              },
              "replicas": {
                "minimum": 1,
                "type": "integer"
              }
            }
          },
          "type": "object"
        }
      }
    },
    "metadata": {
      "type": "object",
      "properties": {
        "annotations": {
          "additionalProperties": true,
          "type": "object",
          "properties": {
            "service.helm.timescale.com/backup-method": {
              "type": "string"
            },
            "service.helm.timescale.com/timescaledb-tune-enabled": {
              "type": "boolean"
            },
            "service.helm.timescale.com/network-policy-enabled": {
              "type": "boolean"
            },
            "service.helm.timescale.com/generate-passwords": {
              "type": "boolean"
            },
            "service.helm.timescale.com/backup-enabled": {
              "type": "boolean"
            }
          }
        }
      }
    }
  }
}
